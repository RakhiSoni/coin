<div class="wrapper wrapper-content">

    <div class="row">
        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
            <div class="ibox">
                <div class="ibox-content" id="history">
                    <div class="box-heading">
                        <a href="{{constants.ACCESSURL}}admin/users">
                            <h2> <img src="img/back-icon.png" class="img-back">User Profile
                            </h2>
                        </a>
                    </div>
                    <div class="border-solid m-b-lg">
                        <a href="" class="downloadLink"></a>
                        <div class="row">
                            <div class="col-md-12">
                                <div class="row">
                                    <div class="col-lg-2 col-md-6 col-sm-6 col-xs-12">
                                        {{#ifCond result.profilePicture '==' ''}}
                                        <img src="{{constants.ASSESSURL}}/img/my-ac-img.png" alt="" class="profile-pic"
                                            style="height: 175px;">
                                        {{else}}
                                        <img src="{{constants.ACCESSURL}}uploads/profile_picture/{{result.profilePicture}}"
                                            alt="" class="kyc-img-size">
                                        {{/ifCond}}
                                    </div>

                                    <div class="col-lg-5 col-md-6 col-sm-6 col-xs-12">
                                        <div class="detail-title-a">
                                            <p><span>First Name:</span>{{result.firstname}} {{result.lastname}}
                                            </p>
                                            <p><span>User
                                                    Name:</span>{{#ifCond result.username '==' '' }}-{{/ifCond}}{{result.username}}
                                            </p>
                                            <p><span>DOB:</span>{{#ifCond result.dob '==' '' }}-{{/ifCond}}{{result.dob}}
                                            </p>
                                            <p><span>Email Address:</span>{{result.email}}</p>
                                            <p><span>Phone Number: </span>{{result.phone}}</p>
                                        </div>
                                    </div>
                                    <div class="col-lg-5 col-md-6 col-sm-6 col-xs-12">
                                        <div class="detail-title-a">
                                            <p>
                                                <span>Company:</span>{{#ifCond result.company '==' '' }}-{{/ifCond}}{{result.company}}
                                            </p>
                                            <p><span>Counrty:</span>{{#ifCond result.kyc.country '==' '' }}-{{/ifCond}}{{result.kyc.country}}
                                            </p>
                                            <p><span>State:</span>{{#ifCond result.kyc.state '==' '' }}-{{/ifCond}}{{result.kyc.state}}
                                            </p>
                                            <p><span>City:</span>{{#ifCond result.kyc.city '==' '' }}-{{/ifCond}}{{result.kyc.city}}
                                            </p>
                                            <p><span>Registered Date:</span>{{result.registeredDate}}</p>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- kyc documents -->
                    <div class="box-heading">
                        <h2 class="m-t-md m-b-md">KYC Documents</h2>
                    </div>
                    <div class="row" id="kyc-doc">
                        {{#ifCond result.kyc.docImage '!=' ''}}
                        <div class="col-lg-3 col-md-4 col-sm-6 col-xs-12">
                            <div class="box">
                                <img src="{{constants.ACCESSURL}}uploads/kyc/docImage/{{result.kyc.docImage}}" alt=""
                                    class="myImg kyc-img-size">
                                <i class="fa fa-search-plus" aria-hidden="true"></i>
                            </div>
                        </div>
                        {{/ifCond}}
                        {{#ifCond result.kyc.docHoldingImage '!=' ''}}
                        <div class="col-lg-3 col-md-4 col-sm-6 col-xs-12">
                            <div class="box">
                                <img src="{{constants.ACCESSURL}}uploads/kyc/docHoldingImage/{{result.kyc.docHoldingImage}}"
                                    alt="" class="myImg kyc-img-size">
                                <i class="fa fa-search-plus" aria-hidden="true"></i>
                            </div>
                        </div>
                        {{/ifCond}}
                        {{#ifCond result.kyc.utilityBill '!=' ''}}
                        <div class="col-lg-3 col-md-4 col-sm-6 col-xs-12">
                            <div class="box">
                                <img src="{{constants.ACCESSURL}}uploads/kyc/utilityBill/{{result.kyc.utilityBill}}"
                                    alt="" class="myImg kyc-img-size">
                                <i class="fa fa-search-plus" aria-hidden="true"></i>
                            </div>
                        </div>
                        {{/ifCond}}
                    </div>
                </div>
            </div>

        </div>
    </div>

    <!-- IMAGE MODAL -->
    <div id="myModal" class="modal kyc myModal">
        <span class="close cursor" onclick="closeModal()">&times;</span>
        <span><i class="fa fa-download  download downloadImage" aria-hidden="true"></i></span>
        <img class="modal-content-A img01">
        <!-- <div id="caption"></div> -->
    </div>