                                                                <textarea class="form-control" placeholder="Message"
                                                                    onkeypress="return minLengthCheck(event)"
                                                                    name="message"></textarea>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="cnt-btn">
                                                        <button class="btn btn-primary" type="submit">Send</button>
                                                    </div>
