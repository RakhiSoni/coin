                                                                <img src="../img/phone-fr-icon.png">
                                                                <input class="form-control" type="text"
                                                                    onkeypress="return minLengthCheck(event)"
                                                                    oninput="return maxLengthCheck(this)" maxlength="13"
                                                                    minlength="7" placeholder="Phone" name="phone"
                                                                    value="{{user.user.phone}}">
                                                            </div>

                                                        </div>
                                                    </div>
